{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run migrations:up && npm run start:backend",
    "start:backend": "ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "build": "npm run clean && tsc -p .",
    "clean": "rimraf dist/",
    "test": "NODE_ENV=test jest -i",
    "test:watch": "NODE_ENV=test jest --watch",
    "tsc": "tsc",
    "tsc:check": "npm run tsc -- --noEmit --skipLibCheck",
    "coverage": "jest --coverage",
    "migrations:create": "ts-node-dev --transpile-only --ignore-watch node_modules src/migrations.ts create",
    "migrations:up": "ts-node-dev --transpile-only --ignore-watch node_modules src/migrations.ts up",
    "migrations:down": "ts-node-dev --transpile-only --ignore-watch node_modules src/migrations.ts down",
    "migrations:test": "npm run migrations:up && npm run migrations:down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.10.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.744.0",
    "@aws-sdk/client-ses": "^3.744.0",
    "@aws-sdk/s3-request-presigner": "^3.744.0",
    "@date-fns/tz": "^1.4.1",
    "axios": "^1.8.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "form-data": "^4.0.4",
    "handlebars": "^4.7.8",
    "inversify": "^6.2.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
